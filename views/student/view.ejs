<div class="profileBox">
    <img src="https://img.icons8.com/ios/150/000000/administrator-male--v1.png"/>
    <h1>Name : <%= one.name %> </h1>
    <h2>Roll No. : <%= one.roll %> </h2>
</div>
<div class="stuTable">
  <table class="table table-striped">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">Subject Name</th>
          <th scope="col">Score (?/100)</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <th scope="row">1</th>
          <td>English</td>
          <td><%= one.english %> </td>
        </tr>
        <tr>
          <th scope="row">2</th>
          <td>Maths</td>
          <td><%= one.math %> </td>
        </tr>
        <tr>
          <th scope="row">3</th>
          <td>Science</td>
          <td><%= one.science %> </td>
        </tr>
        <tr>
          <th scope="row">4</th>
          <td>Social Studies</td>
          <td><%= one.ss %> </td>
        </tr>
        <tr>
          <th scope="row">5</th>
          <td>Language</td>
          <td><%= one.lang %> </td>
        </tr>
      </tbody>
    </table>
    <h1 style="text-align: center;">Status : <%= one.status %> </h1>
</div>